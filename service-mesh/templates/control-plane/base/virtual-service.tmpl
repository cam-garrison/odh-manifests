apiVersion: networking.istio.io/v1alpha3
kind: VirtualService
metadata:
  name: odh-dashboard
  namespace: {{ .AppNamespace }}
spec:
  gateways:
  - odh-gateway
  hosts:
  - "opendatahub.{{ .Domain }}"
  http:
  - route:
    - destination:
        host: odh-dashboard
        port:
          number: 80
